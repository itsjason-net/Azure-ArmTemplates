{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "natGateways_ACSS_lab_ngw_name": {
            "defaultValue": "acss-lab-ngw",
            "type": "String"
        },
        "publicIPAddresses_natgw_pip_name": {
            "defaultValue": "natgw-pip",
            "type": "String"
        },
        "virtualNetworks_ACSS_lab_vnet_name": {
            "defaultValue": "acss-lab-vnet",
            "type": "String"
        },
        "networkSecurityGroups_db_subnet_nsg_name": {
            "defaultValue": "db-subnet-nsg",
            "type": "String"
        },
        "networkSecurityGroups_app_subnet_nsg_name": {
            "defaultValue": "app-subnet-nsg",
            "type": "String"
        },
        "networkSecurityGroups_jumpbox_subnet_nsg_name": {
            "defaultValue": "jumpbox-subnet-nsg",
            "type": "String"
        },
        "virtualNetworks_address_space": {
            "defaultValue": "10.20.0.0/16",
            "type": "String"
        },
        "virtualNetworks_jumpbox_subnet_address_prefix": {
            "defaultValue": "10.20.0.0/24",
            "type": "String"
        },
        "virtualNetworks_app_subnet_address_prefix": {
            "defaultValue": "10.20.1.0/24",
            "type": "String"
        },
        "virtualNetworks_db_subnet_address_prefix": {
            "defaultValue": "10.20.2.0/24",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/networkSecurityGroups",
            "apiVersion": "2022-11-01",
            "name": "[parameters('networkSecurityGroups_app_subnet_nsg_name')]",
            "location": "southcentralus",
            "properties": {
                "securityRules": []
            }
        },
        {
            "type": "Microsoft.Network/networkSecurityGroups",
            "apiVersion": "2022-11-01",
            "name": "[parameters('networkSecurityGroups_db_subnet_nsg_name')]",
            "location": "southcentralus",
            "properties": {
                "securityRules": []
            }
        },
        {
            "type": "Microsoft.Network/networkSecurityGroups",
            "apiVersion": "2022-11-01",
            "name": "[parameters('networkSecurityGroups_jumpbox_subnet_nsg_name')]",
            "location": "southcentralus",
            "properties": {
                "securityRules": []
            }
        },
        {
            "type": "Microsoft.Network/natGateways",
            "apiVersion": "2022-11-01",
            "name": "[parameters('natGateways_ACSS_lab_ngw_name')]",
            "location": "southcentralus",
            "dependsOn": [
                "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_natgw_pip_name'))]"
            ],
            "sku": {
                "name": "Standard",
                "tier": "Regional"
            },
            "properties": {
                "idleTimeoutInMinutes": 4,
                "publicIpAddresses": [
                    {
                        "id": "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_natgw_pip_name'))]"
                    }
                ]
            }
        },
        {
            "type": "Microsoft.Network/publicIPAddresses",
            "apiVersion": "2022-11-01",
            "name": "[parameters('publicIPAddresses_natgw_pip_name')]",
            "location": "southcentralus",
            "sku": {
                "name": "Standard",
                "tier": "Regional"
            },
            "properties": {
                "natGateway": {
                    "id": "[resourceId('Microsoft.Network/natGateways', parameters('natGateways_ACSS_lab_ngw_name'))]"
                },
                "ipAddress": "",
                "publicIPAddressVersion": "IPv4",
                "publicIPAllocationMethod": "Static",
                "idleTimeoutInMinutes": 4,
                "ipTags": [],
                "ddosSettings": {
                    "protectionMode": "VirtualNetworkInherited"
                }
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2022-11-01",
            "name": "[concat(parameters('virtualNetworks_ACSS_lab_vnet_name'), '/jumpbox-subnet')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_ACSS_lab_vnet_name'))]",
                "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_jumpbox_subnet_nsg_name'))]"
            ],
            "properties": {
                "addressPrefix": "[parameters('virtualNetworks_jumpbox_subnet_address_prefix')]",
                "networkSecurityGroup": {
                    "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_jumpbox_subnet_nsg_name'))]"
                },
                "serviceEndpoints": [],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Disabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2022-11-01",
            "name": "[concat(parameters('virtualNetworks_ACSS_lab_vnet_name'), '/app-subnet')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_ACSS_lab_vnet_name'))]",
                "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_app_subnet_nsg_name'))]",
                "[resourceId('Microsoft.Network/natGateways', parameters('natGateways_ACSS_lab_ngw_name'))]"
            ],
            "properties": {
                "addressPrefix": "[parameters('virtualNetworks_app_subnet_address_prefix')]",
                "networkSecurityGroup": {
                    "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_app_subnet_nsg_name'))]"
                },
                "natGateway": {
                    "id": "[resourceId('Microsoft.Network/natGateways', parameters('natGateways_ACSS_lab_ngw_name'))]"
                },
                "serviceEndpoints": [],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Disabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2022-11-01",
            "name": "[concat(parameters('virtualNetworks_ACSS_lab_vnet_name'), '/db-subnet')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_ACSS_lab_vnet_name'))]",
                "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_db_subnet_nsg_name'))]",
                "[resourceId('Microsoft.Network/natGateways', parameters('natGateways_ACSS_lab_ngw_name'))]"
            ],
            "properties": {
                "addressPrefix": "[parameters('virtualNetworks_db_subnet_address_prefix')]",
                "networkSecurityGroup": {
                    "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_db_subnet_nsg_name'))]"
                },
                "natGateway": {
                    "id": "[resourceId('Microsoft.Network/natGateways', parameters('natGateways_ACSS_lab_ngw_name'))]"
                },
                "serviceEndpoints": [],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Disabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2022-11-01",
            "name": "[parameters('virtualNetworks_ACSS_lab_vnet_name')]",
            "location": "southcentralus",
            "dependsOn": [
                "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_app_subnet_nsg_name'))]",
                "[resourceId('Microsoft.Network/natGateways', parameters('natGateways_ACSS_lab_ngw_name'))]",
                "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_db_subnet_nsg_name'))]",
                "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_jumpbox_subnet_nsg_name'))]"
            ],
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "[parameters('virtualNetworks_address_space')]"
                    ]
                },
                "encryption": {
                    "enabled": false,
                    "enforcement": "AllowUnencrypted"
                },
                "subnets": [
                    {
                        "name": "app-subnet",
                        "id": "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('virtualNetworks_ACSS_lab_vnet_name'), 'app-subnet')]",
                        "properties": {
                            "addressPrefix": "10.20.1.0/24",
                            "networkSecurityGroup": {
                                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_app_subnet_nsg_name'))]"
                            },
                            "natGateway": {
                                "id": "[resourceId('Microsoft.Network/natGateways', parameters('natGateways_ACSS_lab_ngw_name'))]"
                            },
                            "serviceEndpoints": [],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Disabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        },
                        "type": "Microsoft.Network/virtualNetworks/subnets"
                    },
                    {
                        "name": "db-subnet",
                        "id": "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('virtualNetworks_ACSS_lab_vnet_name'), 'db-subnet')]",
                        "properties": {
                            "addressPrefix": "10.20.2.0/24",
                            "networkSecurityGroup": {
                                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_db_subnet_nsg_name'))]"
                            },
                            "natGateway": {
                                "id": "[resourceId('Microsoft.Network/natGateways', parameters('natGateways_ACSS_lab_ngw_name'))]"
                            },
                            "serviceEndpoints": [],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Disabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        },
                        "type": "Microsoft.Network/virtualNetworks/subnets"
                    },
                    {
                        "name": "jumpbox-subnet",
                        "id": "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('virtualNetworks_ACSS_lab_vnet_name'), 'jumpbox-subnet')]",
                        "properties": {
                            "addressPrefix": "10.20.0.0/24",
                            "networkSecurityGroup": {
                                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_jumpbox_subnet_nsg_name'))]"
                            },
                            "serviceEndpoints": [],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Disabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        },
                        "type": "Microsoft.Network/virtualNetworks/subnets"
                    }
                ],
                "virtualNetworkPeerings": [],
                "enableDdosProtection": false
            }
        }
    ]
}
